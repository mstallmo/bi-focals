{
  "manifest_version": 2,
  "name": "bi-focals",
  "version": "0.0.1",
  "permissions": ["tabs", "http://*/", "https://*/"],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "content_security_policy": "script-src 'self' 'wasm-eval' 'unsafe-eval'; object-src 'self'"
}
